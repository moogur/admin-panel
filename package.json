{
  "name": "admin-panel",
  "private": true,
  "description": "admin panel for applications",
  "author": "Alexey Dvoretsky",
  "version": "0.0.1",
  "type": "module",
  "license": "UNLICENSED",
  "engines": {
    "node": "16"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build && rm ./dist/index-dev.*",
    "visualizer": "vue-tsc && vite build --mode visualizer",
    "preview": "npm run build && vite preview",
    "lint": "eslint \"{src}/**/*.{ts,json}\" --fix",
    "sonar:publish": "node ./scripts/sonarqube-scanner.js",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "dayjs": "^1.11.7",
    "ky": "^0.33.3",
    "lodash-es": "^4.17.21",
    "pinia": "^2.0.33",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.7",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "eslint": "^8.36.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint-plugin-unicorn": "^46.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.5.0",
    "prettier": "^2.8.7",
    "rollup-plugin-visualizer": "^5.9.0",
    "sass": "^1.60.0",
    "sonarqube-scanner": "^3.0.1",
    "stylelint": "^15.4.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended-scss": "^9.0.1",
    "stylelint-order": "^6.0.3",
    "stylelint-prettier": "^3.0.0",
    "stylelint-scss": "^4.6.0",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-imagemin": "^0.6.1",
    "vue-tsc": "^1.2.0"
  },
  "lint-staged": {
    "./src/**/*.{ts,json}": [
      "npm run lint"
    ]
  }
}
