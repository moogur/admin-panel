{
  "name": "admin-panel",
  "private": true,
  "description": "admin panel for applications",
  "author": "Alexey Dvoretsky",
  "version": "0.0.1",
  "type": "module",
  "license": "UNLICENSED",
  "engines": {
    "node": "18.16.0"
  },
  "scripts": {
    "start": "export $(cat env/.development.env | xargs) && vite",
    "build": "vue-tsc && export $(cat env/.production.env | xargs) && vite build && rm ./dist/index-dev.* && rm -fr ./dist/public",
    "analyze": "vue-tsc && vite build --mode analyze",
    "preview": "npm run build && vite preview",
    "lint": "eslint \"src/**/*.{ts,vue,json}\" --fix",
    "stylelint": "stylelint 'src/**/*.{css,scss}' --fix",
    "sonar:publish": "node ./scripts/sonarqube-scanner.js",
    "pre-commit": "lint-staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "@vuelidate/core": "^2.0.2",
    "@vuelidate/validators": "^2.0.2",
    "dayjs": "^1.11.7",
    "pinia": "^2.0.33",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "eslint": "^8.36.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint-plugin-unicorn": "^46.0.0",
    "eslint-plugin-vue": "^9.10.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.5.0",
    "prettier": "^2.8.7",
    "rollup-plugin-visualizer": "^5.9.0",
    "sass": "^1.60.0",
    "sonarqube-scanner": "^3.0.1",
    "stylelint": "^15.4.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended-scss": "^9.0.1",
    "stylelint-order": "^6.0.3",
    "stylelint-prettier": "^3.0.0",
    "stylelint-scss": "^4.6.0",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-imagemin": "^0.6.1",
    "vue-eslint-parser": "^9.1.1",
    "vue-tsc": "^1.2.0"
  },
  "lint-staged": {
    "./src/**/*.{ts,vue,json}": [
      "npm run lint"
    ],
    "./src/**/*.{css,scss}": [
      "npm run stylelint"
    ]
  }
}
